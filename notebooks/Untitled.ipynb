    "import random\n",
    "class Discrete:\n",
    "\n",
    "    def __init__(self, num_actions:int):\n",
    "        self.n = num_actions\n",
    "\n",
    "    def sample(self):\n",
    "        return random.randint(0, self.n - 1)\n",
    "\n",
    "\n",
    "d = Discrete(4)\n",
    "print(d.sample())"
